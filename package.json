{
  "name": "leveldown-hyper",
  "description": "A Node.js LevelDB binding, primary backend for LevelUP (HyperDex fork)",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/level/leveldown-hyper.git"
  },
  "homepage": "https://github.com/level/leveldown-hyper",
  "keywords": [
    "leveldb",
    "level"
  ],
  "main": "leveldown-hyper.js",
  "dependencies": {
    "abstract-leveldown": "~5.0.0",
    "bindings": "~1.3.0",
    "fast-future": "~1.0.0",
    "nan": "^2.6.2",
    "prebuild-install": "^5.0.0"
  },
  "devDependencies": {
    "du": "~0.1.0",
    "faucet": "0.0.1",
    "iota-array": "~1.0.0",
    "level-community": "^3.0.0",
    "lexicographic-integer": "~1.1.0",
    "mkfiletree": "~1.0.1",
    "monotonic-timestamp": "~0.0.8",
    "node-uuid": "~1.4.3",
    "optimist": "~0.6.1",
    "prebuild": "^7.0.0",
    "prebuild-ci": "^2.2.3",
    "readfiletree": "~0.0.1",
    "remark-cli": "^5.0.0",
    "remark-git-contributors": "^0.2.1",
    "remark-github": "^7.0.3",
    "rimraf": "~2.6.1",
    "slump": "~2.0.0",
    "standard": "^11.0.1",
    "tape": "~4.9.0"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "test": "standard && (tape test/*-test.js | faucet) && prebuild-ci",
    "rebuild": "node-gyp rebuild",
    "prebuild": "prebuild --all --verbose --strip",
    "remark": "remark README.md CONTRIBUTORS.md CHANGELOG.md UPGRADING.md -o"
  },
  "remarkConfig": {
    "plugins": [
      [
        "remark-git-contributors",
        "level-community"
      ],
      [
        "remark-github"
      ]
    ]
  },
  "license": "MIT",
  "gypfile": true,
  "engines": {
    "node": ">=6"
  }
}
